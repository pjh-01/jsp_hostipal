<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>注册</title>
<link rel="stylesheet" href="register.css">
<script type="text/javascript" src="jquery-3.3.1.min.js"></script>
<script>
	var message ='<%=request.getParameter("message")%>';
	if(message == 'yes'){
		alert("注册成功！");
		window.location.href = "login.jsp";
	}
	else if(message == "no"){
		alert("注册失败！");	
	}
	else if(message == "have"){
		alert("该账户已经存在！");
	}
	
	function validate(){
		var account = $('#account').val();
		var phonenum = $('#phonenum').val();
		var password1 = $('#password1').val();
		var password2 = $('#password2').val();
		if(account == ""){
			$('#tips').text("请输入账号！");
			return false;
		}
		else if(phonenum == ""){
			$('#tips').text("请输入手机号码！");
			return false;
		}
		else if(password1 == ""){
			$('#tips').text("请输入想设置的密码！");
			return false;
		}
		else if(password2 == ""){
			$('#tips').text("请再次确认密码！");
			return false;
		}
		else if(!password1.equals(password2)){
			$('#tips').text("两次输入的密码不一致！");
			return false;
		}
	}
	
</script>
